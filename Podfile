# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'MySwift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire' #网络请求
  pod 'SnapKit' #自动布局
  pod 'SwiftyJSON' #处理json
  pod 'HandyJSON', '~> 4.2.0'
  pod 'SVProgressHUD'
  pod 'SDWebImage'
  pod 'LLCycleScrollView'
  pod 'WebViewJavascriptBridge'
  
  #友盟分享
  pod 'UMCCommon'
  pod 'UMCSecurityPlugins'
  pod 'UMCShare/UI'
  pod 'UMCShare/Social/ReducedWeChat' #微信精简版
  pod 'UMCShare/Social/ReducedQQ' #QQ精简版
  pod 'UMCShare/Social/ReducedSina' #新浪精简版

  # Pods for MySwift

  target 'MySwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MySwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
